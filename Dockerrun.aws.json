{
  "AWSEBDockerrunVersion": "1",
  "Image": {
    "Name": "<IMAGE_NAME>:<TAG>",
    "Update": "true"
  },
  "Ports": [
    {
      "ContainerPort": "3000"
    }
  ],
  "Logging": "/var/log/radar-www",
  "environment": [
    {
      "name": "NODE_ENV",
      "value": "dev"
    }
    {
      "name": "WWW_PORT",
      "value": "3001"
    }
    {
      "name": "CONSOLE_LOGLEVEL",
      "value": "debug"
    }
    {
      "name": "API_URL",
      "value": "http://default-environment-in376psmgq.elasticbeanstalk.com/"
    }
  ]
}
